<div>
	<title>Hillside Kitchen & Cellar</title>
</div>
<ul>
	<li><a href="#">Visit</a></li>
	<li><a href="#">Team</a></li>
	<li><a href="#">Menus</a></li>
	<li><a href="/book">Book Now</a></li>
	<li><a href="/">Bookings</a></li>
</ul>
<body class="listings">
<h2>Hello Hillside! Here are all your bookings:</h2>

<% @bookings.each do |booking| %>

<p>
  <div>
    <%= booking.name || 'No Name Provided' %>
  </div>
  <div>
  <%= booking.phone_number || 'No phone number provided' %>
  </div>
  <div>
  <%= booking.number_of_visitors || 'No booking time provided' %>
  </div>
  <div>
  <%= booking.dietary_preferences || 'No dietary preferences provided' %>
  </div>
  <div>
  <%= booking.time || 'Not created' %>
  </div>
  <div>
  <% booking.tables.each do |table| %>
  <%= "table: #{table.id}" %>
  <% end %>
  </div>

</p>
<% end %>

<!-- <h2>And here are their phone numbers:</h2> -->
</body>
